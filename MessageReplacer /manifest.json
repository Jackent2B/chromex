{
    "name": "Message Replacer",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "sketch/index.html",
      "default_title": "Generate a maze!"
    }
}