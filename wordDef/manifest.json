{
    "name": "Word Definition",
    "version": "0.0.1",
    "manifest_version": 2,
    "content_scripts":[{
        "matches":["<all_urls>"],
        "js": ["p5.min.js","content.js"]
    }],
    "background":{
        "scripts": ["background.js"]
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "Get your word def",
        "default_popup": "sketch/popup.html"
    }
}